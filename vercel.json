{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node",
            "config": {
                "installCommand": "npm install",
                "startCommand": "npm start"
            }
        }
    ]
}
